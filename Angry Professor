#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
int T;
    cin >> T;
    int K,N;
    for(int i=0;i<T;i++)
    {
    cin >>N>>K;
    int a,count=0;
    for(int j=0;j<N;j++)
      { cin>>a;
       if(a<=0)
          count++;
      }
       if(count>=K)
           cout<<"NO\n";
       else
           {
           cout<<"YES\n";
       }
    }
    return 0;
}
